unit uEPedidoNaoInformado;

interface

uses
  Sysutils;

type
  EPedidoNaoInformado = class(Exception)
  public
    constructor Create; reintroduce;
  end;

implementation

resourcestring
  Msg = 'Response digest "%s" is not an hexadecimal value.';

constructor EPedidoNaoInformado.Create;
begin
  inherited Create(Msg);
end;

end.
