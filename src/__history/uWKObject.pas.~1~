unit uWKObject;

interface

uses
  Sysutils,
  Classes,
  uWKConnection;

type
  TWKObject = class(TComponent)
  private
    FDB: TWKConnection;
  protected
    property DB: TWKConnection read FDB;
  public
    constructor Create(const ADB: TWKConnection); reintroduce;
  end;

implementation

{ TWKObject }

constructor TWKObject.Create(const ADB: TWKConnection);
begin
  inherited Create(ADB);
  FDB := ADB;
end;

end.
